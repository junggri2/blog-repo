{
  "name": "nest-typescript-starter",
  "private": true,
  "version": "1.0.0",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "cross-env NODE_ENV=production nest start",
    "start:dev": "cross-env NODE_ENV=development nest build --webpack --webpackPath webpack-hmr.config.js nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "cross-env NODE_ENV=production REDIS=redis node dist/src/main",
    "typeormCT": "typeorm migration:create -d ./src/migrationCT -n Content",
    "typeormCT:generate": "ts-node ./node_modules/typeorm/cli.js migration:generate -d ./src/migrationCT -f ./src/content/config/ormconfig-migration.ts -n Content",
    "typeormCT:run": "ts-node ./node_modules/typeorm/cli.js migration:run -c content -f ./src/content/config/ormconfig-migration.ts",
    "typeormCT:revert": "ts-node ./node_modules/typeorm/cli.js migration:revert -c content -f ./src/content/config/ormconfig-migration.ts",
    "typeormCM": "typeorm migration:create -d ./src/migrationCM -n Comment",
    "typeormCM:generate": "ts-node ./node_modules/typeorm/cli.js migration:generate -d ./src/migrationCM -f ./src/comment/config/ormcofig-migration.ts -n Comment",
    "typeormCM:run": "ts-node ./node_modules/typeorm/cli.js migration:run -c comment -f ./src/comment/config/ormcofig-migration.ts",
    "typeormCM:revert": "ts-node ./node_modules/typeorm/cli.js migration:revert -c comment  -f ./src/comment/config/ormcofig-migration.ts",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^7.6.13",
    "@nestjs/config": "^0.6.3",
    "@nestjs/core": "^7.6.13",
    "@nestjs/jwt": "^7.2.0",
    "@nestjs/platform-express": "^7.6.13",
    "@nestjs/serve-static": "^2.1.4",
    "@nestjs/throttler": "^1.1.1",
    "@nestjs/typeorm": "^7.1.5",
    "@types/crypto-js": "^4.0.1",
    "@types/morgan": "^1.9.2",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "connect-redis": "^5.1.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "crypto-js": "^4.0.0",
    "csurf": "^1.11.0",
    "debug": "^4.3.1",
    "dotenv": "^8.2.0",
    "express-session": "^1.17.1",
    "googleapis": "^67.1.1",
    "helmet": "^4.4.1",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "mysql2": "^2.2.5",
    "nest-morgan": "^2.0.0",
    "nodemon": "^2.0.7",
    "redis": "^3.1.2",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.2",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.6",
    "sanitize-html": "^2.3.2",
    "typeorm": "^0.2.31",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.5.6",
    "@nestjs/schematics": "^7.2.7",
    "@nestjs/testing": "^7.6.13",
    "@types/connect-redis": "0.0.16",
    "@types/express": "^4.17.11",
    "@types/express-session": "^1.17.3",
    "@types/jest": "^26.0.20",
    "@types/multer": "^1.4.5",
    "@types/node": "^14.14.31",
    "@types/redis": "^2.8.28",
    "@types/supertest": "^2.0.10",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "run-script-webpack-plugin": "0.0.10",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.2",
    "ts-loader": "^8.0.17",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.5",
    "webpack": "^5.25.0",
    "webpack-node-externals": "^2.5.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
